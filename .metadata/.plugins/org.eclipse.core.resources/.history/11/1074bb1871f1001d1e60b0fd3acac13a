package com.jsp;

public class Pattern123 {
	public static void main(String[] args) {
		int n=5;
		int p=1;
		int k=1;
		int s=2*n-2;
		for(int i=1;i<=2*n-1;i++) {
			for(int j=1;j<=p;j++) {
				System.out.print(j);
				
			}
			for(int j=1;j<=s;j++) {
				System.out.print(j);
			}
			for(int j=1;j<=k;j++) {
				System.out.print(j);
			}
				if(i<n) {
					p++;
					k++;
					s-=2;
				
				}
				else {
					p--;
					k--;
					s+=2;
				}
			System.out.println();
		}
		//System.out.println();
	}

}
