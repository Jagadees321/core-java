package geekforgeek;

import java.util.ArrayList;

public class Laders {
	static ArrayList<Integer> leaders(int arr[], int n){
        // Your code here
        ArrayList<Integer> a=new ArrayList<Integer>();
        if(n==0){
            return a;
        }
        for(int i=0;i<n-1;i++){
            int j=i+1;
           while(j++<n){
               if(arr[i]<arr[j]){
                   continue;
               }
           }
           a.add(arr[i]);
        }
        return a;
    }
	
	public static void main(String[] args) {
		System.out.println(leaders(new int[] {16,17,4,3,5,2}, 6));
	}
}
