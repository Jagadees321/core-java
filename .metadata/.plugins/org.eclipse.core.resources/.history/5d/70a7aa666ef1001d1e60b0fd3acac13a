package com.jsp;

public class Pattern123 {
	public static void main(String[] args) {
		int n=5;
		int p=1;
		int k=1;
		for(int i=1;i<=2*(n-1);i++) {
			for(int j=1;j<=p;j++) {
				System.out.print("*");
				
			}
			for(int j=1;j<=2*(n-2);j++) {
				System.out.print(" ");
			}
			for(int j=1;j<=k;j++) {
				System.out.println("*");
			}
		}
		
	}

}
