package com.jsp.sixtonine;

public class Altprime {

	public static void main(String[] args) {
		int n=23;
		boolean a=isPrime(n);
		System.out.println(a);

	}
    public static boolean isPrime(int n) {
    	if(n<2)return false;
    	for(int i=2;i<=n/2;i++) {
    		if(n%i==0) {
    			return false;
    		}
    		
    	}
    	return true;
    }
}
